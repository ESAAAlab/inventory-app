{
  "name": "inventory-app",
  "version": "1.0.0",
  "author": "melka <melka@blaste.net>",
  "description": "Tool lending app",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "repository": "https://github.com/ESAAAlab/inventory-app",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder ",
    "build:cross": "node .electron-vue/build.js && electron-builder -mwl --x64 --ia32",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "Inventory",
    "appId": "org.blaste.inventory-app",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "chart.js": "^2.7.0",
    "chartkick": "^2.2.4",
    "electron-about-window": "^1.7.1",
    "electron-frameless-frame": "0.0.3",
    "font-awesome": "^4.7.0",
    "ionicons": "^4.2.4",
    "moment": "^2.18.1",
    "pug-plain-loader": "^1.0.0",
    "randomstring": "^1.1.5",
    "vue": "^2.4.3",
    "vue-chartkick": "^0.4.0",
    "vue-electron": "^1.0.6",
    "vue-instant": "^1.0.2",
    "vue-notifications": "^0.9.0",
    "vue-nprogress": "^0.1.5",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^2.1.3",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "devtron": "^1.1.0",
    "electron": "^9.1.0",
    "electron-builder": "^20.19.2",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "ifdef-loader": "^2.0.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.1",
    "multispinner": "^0.2.1",
    "node-sass": "^4.5.3",
    "preprocess-loader": "^0.2.2",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "querystring": "^0.2.0",
    "sass-font-face": "^1.0.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.4.3",
    "webpack": "^4.15.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.19.1"
  }
}
